ALGORITHM sort_algorithm
VAR
    arr:= ARRAY_OF INTEGER;
    i,j,value:= INTEGER;
BEGIN
    FOR i FROM 0 TO arr.length-1 STEP 1 DO
        value= arr[i]
        j:=i
        WHILE ( card > 0 AND arr[i-1] > value) DO
            arr[j]:= arr[j-1]
            j:= j-1
        END_WHILE
        tab[j]:=value
    END_FOR
END